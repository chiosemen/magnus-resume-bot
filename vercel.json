{
  "version": 2,
  "builds": [{ "src": "api/main.py", "use": "@vercel/python" }],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task:/var/task/src",
    "DATABASE_PATH": "/tmp/jobs.db",
    "APP_NAME": "Magnus Resume Bot API",
    "APP_VERSION": "1.0.0"
  }
}
